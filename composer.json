{
    "name": "davidyell/sortable",
    "description": "A small component to implement sortable tables with id's in a Cake view. This allows you to easily attach sorting to any table.",
    "type": "cakephp-plugin",
    "extra": {
        "installer-name": "Sortable"
    },
    "authors": [
        {
            "name": "David Yell",
            "email": "dyell@ukwebmedia.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "joequery/stupid-table-plugin",
                "version": "dev-master",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/joequery/Stupid-Table-Plugin/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/joequery/Stupid-Table-Plugin.git",
                    "reference": "master"
                }
            }

        }
    ],
    "minimum-stability": "dev",
    "require": {
        "composer/installers": "~1.0",
        "joequery/stupid-table-plugin": "dev-master"
    }
}
